extends base.pug
block style
    style
      include ../static/css/home.css
      include ../static/css/donate.css
      include ../static/css/responsive.css
block content
  div.backgrounddonateus.msg
      h1 Donate
      h2 ।। हस्तस्य भूषणं  दानम् ।।
      form(class="form-base" , action='/donate' , method="post" id="form")
        .margin
          .left  Name
          .center :
          .right
            input(type='text' class = 'input' onchange = "nameValidate(event)" name='name' placeholder='Enter your Name')
        .margin
          .left  Email Id
          .center :
          .right
            input(type='email' name='email' class = 'input' onchange = "mailValidate(event)" placeholder='Enter your Email')
        .margin
          .left Phone Number
          .center :
          .right
            input(type='phone' class = 'input' onblur = "numberValidate(event)" name='phone' placeholder='Enter your Contact number')
        .margin
          .text-box
            .left  Address
            .center :
            .right 
              textarea(name='address'  class = 'input' onchange = "addressValidate(event)" cols='30' rows='10')
        .margin
          .left City
