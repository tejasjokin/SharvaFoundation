extends base.pug
block style
  style
    include ../static/css/joinus.css
    include ../static/css/home.css
    include ../static/css/responsive.css
block content
  div.thanks#pop  
    i.fa.fa-times.fa-2x(aria-hidden='true' onclick = 'thanks()')#icon
    div.thankstext 
      p Thank you for joining us at
        b  Sharva Foundation
  div.backgroundjoinus
      h1 Join Us
      form(class="form-base" ,id="form", action='/joinus', method="post")
        .margin
          .left  Name
          .center :
          .right
            input(type='text' name="name" id='name' onchange = "nameValidate(event)" placeholder='Enter your Name')
            h3
        .margin
          .left  Email Id
          .center :
          .right
            input(type='email' name='email' onchange = "mailValidate(event)" placeholder='Enter your Email')
            h3
        .margin
          .left Phone Number
          .center :
          .right
            input(type='phone' name='phone' onblur = "numberValidate(event)" placeholder='Enter your Contact number')
            h3
        .margin
          .text-box
            .left  Address
            .center :
            .right
              textarea(name='address'  onchange = "addressValidate(event)" cols='30' rows='10')
              h3
        .margin
          .left City
          .center :
          .right
